{
  "_type": "export",
  "__export_format": 4,
  "__export_date": "2025-12-26T00:00:00.000Z",
  "__export_source": "github-copilot:churn-alert",
  "resources": [
    {
      "_id": "wrk_churn_alert",
      "_type": "workspace",
      "name": "Churn Alert",
      "scope": "collection"
    },
    {
      "_id": "env_base",
      "_type": "environment",
      "parentId": "wrk_churn_alert",
      "name": "Base Environment",
      "data": {
        "baseUrl": "http://localhost:8080",
        "jwt": ""
      }
    },
    {
      "_id": "fld_auth",
      "_type": "request_group",
      "parentId": "wrk_churn_alert",
      "name": "Auth",
      "environment": {},
      "environmentPropertyOrder": null
    },
    {
      "_id": "fld_churn",
      "_type": "request_group",
      "parentId": "wrk_churn_alert",
      "name": "Churn",
      "environment": {},
      "environmentPropertyOrder": null
    },
    {
      "_id": "fld_batch",
      "_type": "request_group",
      "parentId": "wrk_churn_alert",
      "name": "Batch",
      "environment": {},
      "environmentPropertyOrder": null
    },
    {
      "_id": "fld_analytics",
      "_type": "request_group",
      "parentId": "wrk_churn_alert",
      "name": "Analytics",
      "environment": {},
      "environmentPropertyOrder": null
    },

    {
      "_id": "req_register",
      "_type": "request",
      "parentId": "fld_auth",
      "name": "Register",
      "method": "POST",
      "url": "{{ baseUrl }}/api/auth/register",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"StrongPass123!\",\n  \"fullName\": \"User Name\"\n}"
      },
      "parameters": [],
      "headers": [
        { "name": "Content-Type", "value": "application/json" }
      ]
    },
    {
      "_id": "req_login",
      "_type": "request",
      "parentId": "fld_auth",
      "name": "Login",
      "method": "POST",
      "url": "{{ baseUrl }}/api/auth/login",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"email\": \"admin@local\",\n  \"password\": \"Admin123!\"\n}"
      },
      "parameters": [],
      "headers": [
        { "name": "Content-Type", "value": "application/json" }
      ]
    },

    {
      "_id": "req_predict",
      "_type": "request",
      "parentId": "fld_churn",
      "name": "Predict (single)",
      "method": "POST",
      "url": "{{ baseUrl }}/api/churn/predict",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"tiempo_contrato_meses\": 12,\n  \"retrasos_pago\": 2,\n  \"uso_mensual\": 14.5,\n  \"plan\": \"Premium\"\n}"
      },
      "headers": [
        { "name": "Content-Type", "value": "application/json" },
        { "name": "Authorization", "value": "Bearer {{ jwt }}" }
      ],
      "parameters": []
    },
    {
      "_id": "req_batch_json",
      "_type": "request",
      "parentId": "fld_batch",
      "name": "Predict (batch JSON)",
      "method": "POST",
      "url": "{{ baseUrl }}/api/churn/predict/batch",
      "body": {
        "mimeType": "application/json",
        "text": "[\n  { \n    \"tiempo_contrato_meses\": 12, \n    \"retrasos_pago\": 1, \n    \"uso_mensual\": 10.0, \n    \"plan\": \"Basic\" \n  },\n  { \n    \"tiempo_contrato_meses\": 6, \n    \"retrasos_pago\": 3, \n    \"uso_mensual\": 8.0, \n    \"plan\": \"Premium\" \n  }\n]"
      },
      "headers": [
        { "name": "Content-Type", "value": "application/json" },
        { "name": "Authorization", "value": "Bearer {{ jwt }}" }
      ],
      "parameters": []
    },
    {
      "_id": "req_batch_csv",
      "_type": "request",
      "parentId": "fld_batch",
      "name": "Predict (batch CSV)",
      "method": "POST",
      "url": "{{ baseUrl }}/api/churn/predict/batch/csv",
      "body": {
        "mimeType": "multipart/form-data",
        "params": [
          { "name": "file", "type": "file", "fileName": "C:/path/to/your/file.csv" }
        ]
      },
      "headers": [
        { "name": "Authorization", "value": "Bearer {{ jwt }}" }
      ],
      "parameters": []
    },
    {
      "_id": "req_stats",
      "_type": "request",
      "parentId": "fld_analytics",
      "name": "Stats",
      "method": "GET",
      "url": "{{ baseUrl }}/api/churn/stats",
      "headers": [
        { "name": "Authorization", "value": "Bearer {{ jwt }}" }
      ],
      "parameters": []
    }
  ]
}
